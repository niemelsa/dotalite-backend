generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          Int        @id
  displayName String
  image       String?
  favorites   Favorite[]
}

model Favorite {
  id          Int     @id @default(autoincrement())
  image       String?
  favorite    Int
  userId      Int
  type        String?
  favoritedBy User    @relation(fields: [userId], references: [id])
}
